{
  "name": "super-injector",
  "description": "",
  "version": "0.1.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@types/chai": "*",
    "@types/mocha": "*",
    "@types/node": "*",
    "chai": "*",
    "coveralls": "*",
    "mocha": "*",
    "nyc": "*",
    "source-map-support": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "nyc": {
    "include": [
      "**/*.ts"
    ],
    "exclude": [
      "**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "sourceMap": true,
    "instrument": true
  },
  "scripts": {
    "fix": "tslint --fix --project ./tsconfig.json",
    "build": "npm test && tsc",
    "stop": "killall -KILL node",
    "test": "nyc mocha"
  },
  "bugs": {
    "url": "https://github.com/codyjdalton/super-injector/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codyjdalton/super-injector.git"
  }
}
